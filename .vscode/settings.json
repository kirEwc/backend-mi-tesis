{
    "workbench.iconTheme": "symbols",
    "symbols.files.associations": {
        "*.controller.ts": "nest-controller",
        "*.service.ts": "nest-service",
        "*.module.ts": "nest",
        "*.middleware.ts": "nest-middleware",
        "*.filter.ts": "nest-filter",
        "*.pipe.ts": "nest-pipe",
        "*.gateway.ts": "nest-gateway",
        "*.decorator.ts": "nest-decorator",
        "*.guard.ts": "nest-guard",
        "*.resolver.ts": "nest-resolver",
        "*.spec.ts": "jest",
    },
    "symbols.folders.associations": {
        "entities": "class",
        "dto": "Symbol-typescript",
        "repositories": "Symbol-database",
        "http": "Symbol-container",
        "migrations": "Symbol-tools",
        "modules": "Symbol-core",
        // "test": "",
    }
}